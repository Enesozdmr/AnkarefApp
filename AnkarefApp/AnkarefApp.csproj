    <ItemGroup>
        <_ContentIncludedByDefault Remove="Views\Test\Index.cshtml"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.9"/>
